{
  "name": "@quickdapp/website",
  "version": "3.16.0",
  "description": "QuickDapp marketing website built with Bun, ElysiaJS, and React",
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "type": "module",
  "private": true,
  "main": "src/server/index.ts",
  "engines": {
    "node": ">=22.0.0",
    "bun": ">=1.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run scripts/build.ts",
    "prod": "bun run scripts/prod.ts",
    "test": "NODE_ENV=test bun run scripts/test.ts",
    "test:e2e": "NODE_ENV=test bun run scripts/test-e2e.ts",
    "test:e2e:headed": "NODE_ENV=test bun run scripts/test-e2e.ts --headed",
    "test:e2e:ui": "NODE_ENV=test bun run scripts/test-e2e.ts --ui",
    "typecheck": "tsc --noEmit",
    "lint": "bun run typecheck && biome check .",
    "lint:fix": "bun run typecheck && biome check --write .",
    "format": "biome format --write .",
    "gen": "bun run scripts/gen.ts",
    "db": "bun run scripts/db.ts",
    "fetch-docs": "bun run scripts/fetch-docs.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^5.0.0",
    "@hiddentao/zip-json": "^1.1.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.0.0-alpha.19",
    "@types/bun": "latest",
    "@types/hast": "^3.0.0",
    "@types/lunr": "^2.3.7",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^4.3.4",
    "remark-stringify": "^11.0.0",
    "strip-markdown": "^6.0.0",
    "tailwindcss": "^4.0.0-alpha.19",
    "typescript": "^5.6.3",
    "vite": "^6.0.3"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/static": "^1.3.0",
    "@graphql-tools/schema": "^10.0.25",
    "@hiddentao/logger": "^1.3.1",
    "@octokit/rest": "^21.0.0",
    "@posthog/react": "^1.7.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/node": "^10.19.0",
    "@sentry/react": "^10.38.0",
    "@shikijs/core": "^3.21.0",
    "@shikijs/rehype": "^3.21.0",
    "@tanstack/react-query": "^5.62.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "cron-schedule": "^5.0.4",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "elysia": "^1.3.8",
    "env-var": "^7.5.0",
    "glob": "^11.0.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.1.2",
    "graphql-scalars": "^1.24.2",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^5.15.1",
    "gray-matter": "^4.0.3",
    "hast-util-to-jsx-runtime": "^2.3.0",
    "jose": "^6.0.12",
    "json-bigint-patch": "^0.0.8",
    "lucide-react": "^0.460.0",
    "lunr": "^2.3.9",
    "nanoid": "^5.1.6",
    "postgres": "^3.4.7",
    "posthog-js": "^1.347.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.28.0",
    "rehype-react": "^8.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "semver": "^7.7.3",
    "tailwind-merge": "^2.5.4",
